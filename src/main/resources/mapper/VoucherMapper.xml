<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luke.playhub.mapper.VoucherMapper">
    <select id="selectById" parameterType="long" resultType="com.luke.playhub.entity.Voucher">
        SELECT id, shop_id, stock, create_time, update_time FROM voucher WHERE id = #{id}
    </select>
</mapper>
